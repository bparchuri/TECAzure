{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-Poc-Tec"
		},
		"ADLSGen2_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'ADLSGen2'"
		},
		"SQLPocTec_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SQLPocTec'"
		},
		"SQLPocTec_servicePrincipalKey": {
			"type": "secureString",
			"metadata": "Secure string for 'servicePrincipalKey' of 'SQLPocTec'"
		},
		"TecDevCRM_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'TecDevCRM'"
		},
		"ADLSGen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sapoctec.dfs.core.windows.net"
		},
		"ADLSGen2_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "6e5433e0-76fc-44b7-b71b-0e5446a3fa94"
		},
		"ADLSGen2_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "b4537ba3-1639-42cb-9143-8bfbcb683e79"
		},
		"SQLPocTec_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "b4537ba3-1639-42cb-9143-8bfbcb683e79"
		},
		"SQLPocTec_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "6e5433e0-76fc-44b7-b71b-0e5446a3fa94"
		},
		"TecDevCRM_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "Bhargav.Parchuri@letec.be"
		},
		"HttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://opendata.tec-wl.be/Current%20GTFS/TEC-GTFS.zip"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLSGen2_servicePrincipalKey": {
						"value": "[parameters('ADLSGen2_servicePrincipalKey')]"
					},
					"SQLPocTec_connectionString": {
						"value": "[parameters('SQLPocTec_connectionString')]"
					},
					"SQLPocTec_servicePrincipalKey": {
						"value": "[parameters('SQLPocTec_servicePrincipalKey')]"
					},
					"TecDevCRM_password": {
						"value": "[parameters('TecDevCRM_password')]"
					},
					"ADLSGen2_properties_typeProperties_url": {
						"value": "[parameters('ADLSGen2_properties_typeProperties_url')]"
					},
					"ADLSGen2_properties_typeProperties_tenant": {
						"value": "[parameters('ADLSGen2_properties_typeProperties_tenant')]"
					},
					"ADLSGen2_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('ADLSGen2_properties_typeProperties_servicePrincipalId')]"
					},
					"SQLPocTec_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('SQLPocTec_properties_typeProperties_servicePrincipalId')]"
					},
					"SQLPocTec_properties_typeProperties_tenant": {
						"value": "[parameters('SQLPocTec_properties_typeProperties_tenant')]"
					},
					"TecDevCRM_properties_typeProperties_username": {
						"value": "[parameters('TecDevCRM_properties_typeProperties_username')]"
					},
					"HttpServer1_properties_typeProperties_url": {
						"value": "[parameters('HttpServer1_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		}
	]
}