			"type": "string",
			"defaultValue": "https://sapoctec.dfs.core.windows.net"
		},
		"AzureFileStorage1_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "\\\\sapoctec.file.core.windows.net\\adls-poc"
		},
		"AzureFileStorage1_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "Azure\\sapoctec"
		},
		"HttpServer1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "http://opendata.tec-wl.be/Current%20GTFS/TEC-GTFS.zip"
		},
		"KeyVaultPoc_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://keyvault-Poc-tec.vault.azure.net/"
		},
