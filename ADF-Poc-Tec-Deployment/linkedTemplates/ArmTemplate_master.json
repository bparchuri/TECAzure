			"type": "string",
			"defaultValue": "SQL19"
		},
		"SQLPocTec_properties_typeProperties_servicePrincipalId": {
			"type": "string",
			"defaultValue": "b4537ba3-1639-42cb-9143-8bfbcb683e79"
		},
		"SQLPocTec_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "6e5433e0-76fc-44b7-b71b-0e5446a3fa94"
		},
		"TecDevCRM_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "Bhargav.Parchuri@letec.be"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
