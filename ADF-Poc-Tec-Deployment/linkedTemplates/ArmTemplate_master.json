{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "ADF-Poc-Tec-Deployment"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureDatabricks1_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks1'"
		},
		"AzureFileStorage1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureFileStorage1'"
		},
		"ADLSCOD_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sapoctec.dfs.core.windows.net"
		},
		"ADLSCOD_properties_typeProperties_tenant": {
			"type": "string",
			"defaultValue": "6e5433e0-76fc-44b7-b71b-0e5446a3fa94"
