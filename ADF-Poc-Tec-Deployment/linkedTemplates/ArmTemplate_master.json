					},
					"ADLSCOD_properties_typeProperties_url": {
						"value": "[parameters('ADLSCOD_properties_typeProperties_url')]"
					},
					"ADLSCOD_properties_typeProperties_tenant": {
						"value": "[parameters('ADLSCOD_properties_typeProperties_tenant')]"
					},
					"ADLSCOD_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('ADLSCOD_properties_typeProperties_servicePrincipalId')]"
					},
					"ADLSGen2_properties_typeProperties_url": {
						"value": "[parameters('ADLSGen2_properties_typeProperties_url')]"
					},
					"ADLSGen2_properties_typeProperties_tenant": {
						"value": "[parameters('ADLSGen2_properties_typeProperties_tenant')]"
					},
					"ADLSGen2_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('ADLSGen2_properties_typeProperties_servicePrincipalId')]"
					},
					"SQLPocTec_properties_typeProperties_connectionString_secretName": {
						"value": "[parameters('SQLPocTec_properties_typeProperties_connectionString_secretName')]"
					},
					"SQLPocTec_properties_typeProperties_servicePrincipalId": {
						"value": "[parameters('SQLPocTec_properties_typeProperties_servicePrincipalId')]"
					},
					"SQLPocTec_properties_typeProperties_tenant": {
						"value": "[parameters('SQLPocTec_properties_typeProperties_tenant')]"
					},
					"TecDevCRM_properties_typeProperties_username": {
						"value": "[parameters('TecDevCRM_properties_typeProperties_username')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_0')]"
			]
		},
		{
			"name": "ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'ArmTemplate_1')]"
			]
		},
		{
			"name": "ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
